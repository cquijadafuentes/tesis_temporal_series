red = "#FF0000"; green = "#00FF00"; blue = "#0000FF"; skyblue = "#87CEEB"; black = "#000000"; darkgreen = "#006400"; darkred = "#8b0000";
set grid ytics
set key out horiz
set terminal png size 800,400

set ylabel "I de Moran"
set xlabel "Instantes"

set yrange [-1:1]


set output 'img_i_moran_APCP_full.png'
set title 'Evolucion Moran I APCP'
